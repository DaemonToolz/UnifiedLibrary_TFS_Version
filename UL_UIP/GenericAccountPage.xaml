<Page x:Class="UL_UIP.GenericAccountPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:UL_UIP"
      mc:Ignorable="d" 
      d:DesignHeight="312" d:DesignWidth="600"
      Title="GenericAccountPage">

    <Grid x:Name="MainGrid" >
        <Label x:Name="TitleLabel" Foreground="White" Content="" HorizontalAlignment="Left" VerticalAlignment="Top" FontWeight="Bold" FontSize="14" Width="600" Height="36" BorderThickness="0,0,0,2" BorderBrush="White"/>
        <Label x:Name="AccountType" Foreground="White" Content="" HorizontalAlignment="Left" VerticalAlignment="Top" FontWeight="Bold" Width="600" Height="29" BorderThickness="0" BorderBrush="White" Margin="0,41,0,0" Background="{x:Null}"/>

    </Grid>
</Page>
